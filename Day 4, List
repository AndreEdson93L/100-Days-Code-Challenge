#Don't memorize, make things work

python_docs = {"list_docs: " : "https://docs.python.org/3/tutorial/datastructures.html?highlight=list", "list_of_US_states: " : "https://en.wikipedia.org/wiki/List_of_U.S._states_by_date_of_admission_to_the_Union", "ask_python: " : "https://www.askpython.com/python-modules/python-random-module-generate-random-numbers-sequences"}

states_of_america = ["Delaware", "Pennsylvania", "New Jersey", "Georgia", "Connecticut", "Massachusetts", "Maryland", "South Carolina", "New Hampshire", "Virginia", "New York", "North Carolina", "Rhode Island", "Vermont", "Kentucky", "Tennessee", "Ohio", "Louisiana", "Indiana", "Mississippi", "Illinois", "Alabama", "Maine", "Missouri", "Arkansas", "Michigan", "Florida", "Texas", "Iowa", "Wisconsin", "California", "Minnesota", "Oregon", "Kansas", "West Virginia", "Nevada", "Nebraska", "Colorado", "North Dakota", "South Dakota", "Montana", "Washington", "Idaho", "Wyoming", "Utah", "Oklahoma", "New Mexico", "Arizona", "Alaska", "Hawaii"]

print(states_of_america[0]) # First
print(states_of_america[len(states_of_america) - 1]) # Last
print(states_of_america[-1]) # Last
print(states_of_america[-2]) # Second last

dirty_dozen = ["Strawberries", "Spinach", "Kale", "Nectarines", "Apples", "Grapes", "Peaches", "Cherries", "Pears", "Tomatoes", "Celery", "Potatoes"]

dirty_dozen.append("The Third!")

print(len(dirty_dozen))

for key, value in python_docs.items():
  print(key, value)

dirty_dozen.extend(["Random Word", "Random World", "Not So Random Anymore"])
print(len(dirty_dozen))
